{
  "clusters": [
    "10.200.120.200",
    "10.200.120.202"
  ],
  "global": {
    "namespace": "monitoring",
    "prometheus": {
      "scrape_interval": "15s",
      "evaluation_interval": "15s",
      "retention": "7d",
      "storage_size": "10Gi",
      "ports": {
        "metrics": 9100,
        "api": 9101,
        "node_exporter": 9100,
        "c_metrics": 9102,
        "c_advisor": 9103
      }
    }
  },
  "auth": {
    "username": "admin"
  },
  "dashboard": {
    "port": 8080,
    "theme": "dark"
  }
}
